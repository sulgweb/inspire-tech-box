{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ],
      "@captureWin/*": [
        "src/renderer/captureWin/src/*"
      ],
      "@floatWin/*":[
        "src/renderer/floatWin/src/*"
      ],
      "@homeWin/*":[
        "src/renderer/homeWin/src/*"
      ]
    }
  }
}
